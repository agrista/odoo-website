<odoo>
    <record id="menu_tree" model="ir.ui.view">
        <field name="name">menu.tree.inherit</field>
        <field name="model">website.menu</field>
        <field name="inherit_id" ref="website.menu_tree"/>
        <field name="arch" type="xml">
            <field name="new_window" position="after">
                <field name="user_logged_in"/>
                <field name="user_not_logged_in"/>
            </field>
        </field>
    </record>
</odoo>
